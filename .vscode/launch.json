    "version": "0.2.0",
    "configurations": [


        {
            "name": "dagit: localhost",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}/",
                    "remoteRoot": "/opt/dagster-template/",
                }
